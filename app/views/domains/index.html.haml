%div{class: 'container'}
  %div{class: 'panel-group', id: 'img-json', role: 'tablist'}
    %div{class: 'panel panel-primary'}
      %div{class: 'panel-heading', role: 'tab', id: 'heading-img-json'}
        %h4{class: 'panel-title'}
          %a{role: 'button', 'data-toggle': 'collapse', 'data-parent': 'img-json', href: '#content-img-json'}
            Image Paths (
            = @images.length
            images
            \)
      %div{id: 'content-img-json', class: 'panel-collapse collapse in', role: 'tabpanel'}
        %pre
          = @images.join("\n")
%div{class: 'container'}
  - @images.each do |_path|
    %img{src: "/assets/#{_path}", class: 'img-cell'}

= stylesheet_link_tag 'style'